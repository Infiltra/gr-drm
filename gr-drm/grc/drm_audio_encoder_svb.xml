<block>
  <name>Audio Encoder</name>
  <key>drm_audio_encoder_svb</key>
  <category>DRM</category>
  <import>import drm</import>
  <make>drm.audio_encoder_svb($tp)</make>
  <param>
    <name>Transmit Parameters</name>
    <key>tp</key>
    <type>raw</type>
  </param>
  <param>
    <name>Output Vector Length</name>
    <key>len_out</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
    <vlen>$len_out</vlen>
  </source>
</block>

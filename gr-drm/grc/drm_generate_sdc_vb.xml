<block>
  <name>SDC Generator</name>
  <key>drm_generate_sdc_vb</key>
  <category>DRM</category>
  <import>import drm</import>
  <make>drm.generate_sdc_vb($tp)</make>
  <param>
    <name>Transmit Parameters</name>
    <key>tp</key>
    <type>raw</type>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
    <vlen>tp.sdc().L()</vlen>
  </source>
</block>
